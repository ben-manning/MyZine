<div>
  <%= render 'form' %>
</div>

<h1>Your Articles!!</h1>

<div>
  <% @articles.each do |article| %>
    <h3><%= link_to article.title, article.url %></h3><br>
    <p><%= link_to 'Delete', user_favorite_path(current_user, article),
    											 method: :delete,
    											 data: { confirm: 'Are you sure?' } %></p>
  <% end %>
</div>

