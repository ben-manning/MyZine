<div>
  <%= render 'form' %>
</div>

<div class="jumbotron">
  <div class="results">
    <h2>Results</h2>
  </div>
<ul>
	<% @articles.each do |article| %>
		<li>
			<h3><%= link_to article["webTitle"], article["webUrl"] %></h3>
			<%= link_to 'Favorite', user_favorites_path(user_id: current_user.to_param, api_url: article["apiUrl"]), remote: true, method: :post, class: 'btn btn-primary' %>
		</li>
	<% end %>
</ul>
</div>
